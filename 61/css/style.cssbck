.plantilla6_bot-background
{
    background-color: #000000;
    height: 100%;
    width: 100%;
    position: absolute;
    z-index: 4;

}

.plantilla6_mail
{
    width: 100%;
    height: 41%;
    
    font-size: 1.35em;
    color: white;
    font-weight: 600;
    font-family: "Open Sans";
    text-align: center;
    vertical-align: middle;
    display: flex;
    justify-content: center;
    align-items: center;

    box-sizing: border-box;
    padding-top: 1%;

    z-index: 5;
}
.plantilla6_number-container
{
    width: 48%;
    height: 60%;
    display: flex;
    justify-content: center;

    padding-bottom: 5%;
    z-index: 5;
}
.plantilla6_number-container .plantilla6_logo
{

    
    height: 100%;
    width: 10%;
    background-image: url(../media/WHBot.png);
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center;
}
.plantilla6_number-container .plantilla6_number-text
{
    font-size: 1.35em;
    line-height: 1.8em;
    color: white;
    font-weight: 400;
    font-family: "Open Sans";
    text-align: center;
}

.plantilla6_top-left-animation
{
    animation-name: plantilla6_top-left-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_top-left-animation
{
    0%{transform: translateX(-50em);}
    5%{transform: translateX(0);}
    
    95.3%{transform: translateX(0);}
    100%{transform: translateX(50em);}

    
}

.plantilla6_top-right-animation
{
    animation-name: plantilla6_top-right-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_top-right-animation
{
    0%{transform: translateX(50em);}
    5%{transform: translateX(0);}
    
    95.3%{transform: translateX(0);}
    100%{transform: translateX(50em);}

    
}


.plantilla6_info-animation
{
    animation-name: plantilla6_info-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_info-animation
{
    0%{opacity:0;}
    5%{opacity: 0;}
    10%{opacity: 1;}

    92.5%{opacity: 1;}
    100%{opacity: 0;}

}

.plantilla6_button-animation
{
    animation-name: plantilla6_button-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_button-animation
{
    0%{transform: translateY(10em);}
    2.6%{transform: translateY(10em);}
    6.6%{transform: translateY(0);}
    
    92.5%{transform: translateX(0);}
    100%{transform: translateX(21em);}

    
}



.plantilla6_bot-animation
{
    animation-name: plantilla6_bot-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_bot-animation
{
    0%{transform: translateX(-50em);}
    2.8%{transform: translateX(0);}
    
    97%{transform: translateX(0);}
    100%{transform: translateX(50em);}

    
}


.plantilla6_bot-mail-animation
{
    animation-name: plantilla6_bot-mail-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_bot-mail-animation
{
    0%{transform: translateY(4em);}
    2.8%{transform: translateY(4em);}
    8.3%{transform: translateY(0);}
    
    94.16%{transform: translateY(0);}
    100%{transform: translateY(4em);}

    
}

.plantilla6_bot-number-animation
{
    animation-name: plantilla6_bot-number-animation;
    animation-duration: 20s;
    animation-fill-mode: forwards;
}

@keyframes plantilla6_bot-number-animation
{
    0%{transform: translateY(3em);}
    8.6%{transform: translateY(3em);}
    11.6%{transform: translateY(0);}
    
    94.16%{transform: translateY(0);}
    100%{transform: translateY(6em);}

    
}





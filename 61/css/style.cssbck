}

.plantilla1_container-bot .plantilla1_logo
{

    height: 100%;
    width: 15%;

    background-image: url("../media/Logo\ Negro.png");
    
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center;
}

.plantilla1_down-tag
{
    height: 70%;
    width: 100%;

    display: flex;
    flex-direction: row-reverse;

    align-items: center;
}

.plantilla1_down-tag .plantilla1_details
{
    height: 100%;
    width: 33%;

    display: flex;
    flex-direction: column;
    

    
    color: #000000;
    font-family: 'PT Sans';
    
    text-align: right;

    padding-right: 6%;
    box-sizing: border-box;
}

.plantilla1_down-tag .plantilla1_details .plantilla1_cents
{  
    
    font-weight: 700;
    /* padding-top: 10%; */
    font-size: 40px;
    

}

.plantilla1_down-tag .details .plantilla1_units
{  
    font-style: italic;
    font-size: 30px;

    /* padding-bottom: 16%; */
    box-sizing: border-box;
    

}

.plantilla1_down-tag .plantilla1_price
{
    height: 100%;
    min-width: 34%;

    display: flex;
    justify-content: flex-end;    
    
    
    letter-spacing: -2px;

    font-size: 132px;
    
    color: #000000;
    font-family: 'PT Sans';
    font-weight: 700;

    text-align: right;

    padding-left: 6%;
    box-sizing: border-box;

    line-height: 91px;
    

}

.plantilla1_old-title
{
    font-size: 32px;
    
    color: #000000;
    font-weight: 400;
    font-family: "Open Sans";
    text-align: right;
    
}



/**ANIMACIONES*/
.plantilla1_top-animation
{
    animation-name: plantilla1_top-animation;
    animation-duration: 12s;
    animation-fill-mode: forwards;
}

@keyframes plantilla1_top-animation
{
    0%{transform: translateY(-7em);}
    12%{transform: translateY(0);}
    
    91.6%{transform: translateY(0);}
    100%{transform: translateY(-7em);}

    
}

.plantilla1_left-img-animation
{
    animation-name: plantilla1_left-img-animation;    
    animation-duration: 12s;

    animation-fill-mode: forwards;
}
@keyframes plantilla1_left-img-animation
{
    0%{transform:translateX(-25em);}
    2.5%{transform:translateX(-25em);}
    12.5%{transform:translateX(0);}

    91.6%{transform:translateX(0);}
    100%{transform:translateX(-25em);}
    
}






.plantilla1_right-title-animation
{
    animation-name: plantilla1_right-title-animation;
    
    animation-duration: 12s;
    animation-fill-mode: forwards;
    
}
@keyframes plantilla1_right-title-animation
{
    
    0%{transform:translateX(13em);}
    3.33%{transform:translateX(13em);}
    16.6%{transform:translateX(0);}
    
    91.6%{transform:translateX(0);}
    100%{transform:translateX(13em);}
    
}

.plantilla1_right-description-animation
{
    animation-name: plantilla1_right-description-animation;
    
    animation-duration: 12s;
    animation-fill-mode: forwards;
    
}
@keyframes plantilla1_right-description-animation
{
    
    0%
    {
        transform:translateX(25em);
        opacity:0;
    }
    12%
    {
        transform:translateX(25em);
        opacity: 0;
    }
    24%
    {
        transform:translateX(0);
        opacity: 100%;
    }
    
    91.6%
    {
        transform:translateX(0);
    }
    100%
    {
        transform:translateX(25em);
    }
    
}



.plantilla1_new-price-animation
{
    
    transition: transform 300ms;
    animation-name: plantilla1_new-price-animation;
    animation-duration: 12s;

    animation-fill-mode: forwards;

}

@keyframes plantilla1_new-price-animation
{

    0%{transform:scale(0.001);}
    16%{transform:scale(0.001);}
    27%{transform:scale(1);}
    91.6%{transform:scale(1);}

    100%{transform:scale(0);}

    
} 





